{
  "name": "react-template-singlepage",
  "version": "1.1.0",
  "homepage": ".",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npm run dll & npm start",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --config config/webpack.dev.js & npm run tslint:watch",
    "dll": "webpack --config config/dll.config.js --mode=development --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "format": "prettier --write \"./src/**/*.{js,jsx,json}\"",
    "tslint": "tsc --noEmit",
    "tslint:watch": "tsc --noEmit -w",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "stylelint": "stylelint \"./src/**/*.{css,less}\" --fix",
    "lint": "run-p tslint eslint stylelint",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && git add ."
    }
  },
